{
  "name": "admin-service",
  "version": "1.0.0",
  "description": "Admin microservice for AIOutlet platform.",
  "main": "src/api/server.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node src/api/server.js",
    "start:api": "node src/api/server.js",
    "start:consumer": "node src/consumer/consumer.js",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:consumer\"",
    "dev": "nodemon src/api/server.js",
    "dev:api": "nodemon src/api/server.js",
    "dev:consumer": "nodemon src/consumer/consumer.js",
    "dev:both": "concurrently \"npm run dev:api\" \"npm run dev:consumer\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aioutlet/admin-service.git"
  },
  "keywords": [
    "aioutlet",
    "microservice",
    "node",
    "express",
    "mongo"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aioutlet/admin-service/issues"
  },
  "homepage": "https://github.com/aioutlet/admin-service#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^30.0.2",
    "jest": "^30.0.2",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "axios": "^1.10.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  }
}
